<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<remote name="github" fetch="https://github.com" pushurl="ssh://git@github.com"/>

	<project path="ide" name="aurora-fw/ide" >
		<linkfile src=".vscode/launch.json" dest=".vscode/launch.json" />
		<linkfile src=".vscode/tasks.json" dest=".vscode/tasks.json" />
		<linkfile src=".vscode/settings.json" dest=".vscode/settings.json" />
		<linkfile src=".vscode/c_cpp_properties.json" dest=".vscode/c_cpp_properties.json" />
		<linkfile src=".vscode/.cmaketools.json" dest=".vscode/.cmaketools.json" />
	</project>
	<project path="docker/aurora" name="aurora-fw/docker" />
	<project path="web/status" name="aurora-fw/status" />
	<project path="web/website" name="aurora-fw/aurora-fw.github.io" />
	<project path="docs" name="aurora-fw/docs" >
		<linkfile src="INSTALL.md" dest="INSTALL.md" />
	</project>
	<project path="build" name="aurora-fw/build" >
		<linkfile src="LICENSE" dest="LICENSE" />
		<linkfile src="CMakeLists.txt" dest="CMakeLists.txt" />
		<linkfile src="dub.json" dest="dub.json" />
	</project>
	<project path="cmake/freeimage" name="aurora-fw/cmake_freeimage" />
	<project path="cmake/portaudio" name="aurora-fw/cmake_portaudio" />
	<project path="cmake/sndfile" name="aurora-fw/cmake_libsndfile" />
	<project path="cmake/vulkan" name="aurora-fw/cmake_vulkan" />
	<project path="cmake/osmesa" name="aurora-fw/cmake_osmesa" />
	<project path="cmake/mir" name="aurora-fw/cmake_mir" />
	<project path="cmake/wayland" name="aurora-fw/cmake_wayland" />
	<project path="cmake/xkb" name="aurora-fw/cmake_xkb" />
	<project path="cmake/aurora" name="aurora-fw/cmake_aurora" />
	<project path="cmake/d" name="aurora-fw/cmake_d" />
	<project path="toolchain/fastmake" name="aurora-fw/fastmake">
	<project path="toolchain/aufw-glloader_legacy" name="aurora-fw/aufw-glloader_legacy">
	<project path="toolchain/aufw-glloader" name="aurora-fw/aufw-glloader">
	<project path="toolchain/afwmmanager" name="aurora-fw/afwmmanager">

	<project path="aurora" name="aurora-fw/aurora" >
		<linkfile src="README.md" dest="README.md" />
	</project>

	<include name="github/project/modules.xml" />
	<include name="github/project/binds.xml" />
	<include name="github/project/apps.xml" />
</manifest>
